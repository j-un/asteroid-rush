{
  "name": "asteroid-weave",
  "version": "1.0.0",
  "description": "3D Space Endless Runner",
  "type": "module",
  "main": "dist/script.js",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "serve": "vite preview",
    "clean": "rm -rf dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"{src,test}/**/*.ts\""
  },
  "keywords": [
    "game",
    "three.js",
    "typescript",
    "3d"
  ],
  "author": "Jun Tanaka",
  "license": "MIT",
  "dependencies": {
    "three": "^0.177.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/three": "^0.177.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  }
}
